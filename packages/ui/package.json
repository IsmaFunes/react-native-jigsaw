    "@draftbit/core": "40.5.1",
    "@draftbit/native": "40.5.1"
  },
  "devDependencies": {
    "utility-types": "^3.0.0",
    "react": "16.13.1",
    "react-native": "~0.63.2",
    "rimraf": "^3.0.2",
    "typescript": "4.0.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "@react-native-community/bob": {
